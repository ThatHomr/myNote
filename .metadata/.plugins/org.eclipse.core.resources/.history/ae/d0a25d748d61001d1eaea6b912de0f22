import java.util.Scanner;

public class Main {
	public static void main(String[] args)  {
		Scanner sc = new Scanner(System.in);
		
		String s = sc.next();
		String dummy = sc.nextLine();
		
		String answer = "";
		
		String[] str = s.split(" ");
		int length = str.length;
		
		for (int i = 0; i < length; i++) {
			System.out.println("str[" + i + "] = " + str[i]);
		}
		
		
		
		
		sc.close();
	}
		
}

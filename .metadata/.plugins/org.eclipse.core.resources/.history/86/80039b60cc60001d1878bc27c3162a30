public class Main {
	public static void main(String[] args)  {

		int[] num_list = {1, 2, 3, 4, 5, 6, 7, 8};
		
		int n = 2;
		
		
		int length = num_list.length;
		int len = length / n;
		int[][] answer = new int[len][n];
		int num = 0;
		int len2 = 0;
		
		for (int i = 0; i < len; i++) {
			for (int j = 0; j < n; j++) {
				answer[i][j] = num_list[num];
				num++;
				System.out.println(answer[i][j]);
			}
		}
		
		
		
		
	}
		
}
